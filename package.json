{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:css": "npx tailwindcss -i ./src/styles/style.css -o ./public/css/style.css --minify",
    "fix:html": "cp -r dist/html/* dist/",
    "predeploy": "npm run build && npm run fix:html",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.1",
    "vite": "^6.2.0"
  },
  "dependencies": {
    "socket.io-client": "^4.8.1"
  }
}
